{
  "name": "docs",
  "version": "0.0.0",
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version",
    "lint": "markdownlint --config markdownlint.json docs/**/*.md docs/*.md",
    "format": "prettier --write docs/*.md docs/**/*.md",
    "format-js": "prettier --write pages/**/*.js",
    "format-css": "prettier --write static/css/*.css",
    "check-format": "prettier --check docs/*.md docs/**/*.md",
    "check-spelling": "mdspell --report --en-us -n \"docs/**/*.md\""
  },
  "devDependencies": {
    "markdown-spellcheck": "1.3.1",
    "markdownlint-cli": "0.38.0",
    "prettier": "3.2.4"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.1",
    "@docusaurus/plugin-client-redirects": "^3.1.0",
    "@docusaurus/plugin-sitemap": "^3.1.0",
    "@docusaurus/preset-classic": "^3.0.1",
    "@mdx-js/react": "^3.0.0",
    "@svgr/webpack": "^8.1.0",
    "clsx": "^2.1.0",
    "file-loader": "^6.2.0",
    "mermaid": "10.7.0",
    "npm": "^10.3.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "4.1.5",
    "remark-emoji": "4.0.1",
    "url-loader": "^4.1.1"
  }
}
