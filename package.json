{
  "name": "docs",
  "version": "0.0.0",
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version",
    "lint": "markdownlint --config markdownlint.json docs/**/*.md docs/*.md",
    "format": "prettier --write docs/*.md docs/**/*.md",
    "format-js": "prettier --write pages/**/*.js",
    "format-css": "prettier --write static/css/*.css",
    "check-format": "prettier --check docs/*.md docs/**/*.md",
    "check-spelling": "mdspell --report --en-us -n \"docs/**/*.md\""
  },
  "devDependencies": {
    "markdown-spellcheck": "1.3.1",
    "markdownlint-cli": "0.32.2",
    "prettier": "2.8.1"
  },
  "dependencies": {
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/plugin-client-redirects": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@mdx-js/react": "1.6.21",
    "@svgr/webpack": "^6.5.1",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "url-loader": "^4.1.1"
  }
}
